#%Module1.0

set version 2.3.3
set moduleInfo "SystemC library v$version"

proc ModulesHelp { } {
    global moduleInfo
    puts stderr "set up environment for $moduleInfo"
}

module-whatis $moduleInfo

set systemc_root /workspace/system/systemc/systemc-$version
setenv SYSTEMC_ROOT $systemc_root
prepend-path LD_LIBRARY_PATH $systemc_root/lib-linux64

conflict systemc
